@model IEnumerable<FARMC.Models.Fisherfolk>

@{
    ViewBag.Title = "Fisherfolks List";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FisherId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FisherId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FisherfolkId }) |
            @Html.ActionLink("Details", "Details", new { id=item.FisherfolkId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FisherfolkId })
        </td>
    </tr>
}

</table>
